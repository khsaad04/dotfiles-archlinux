/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME add ~/.config/waybar
/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME add ~/.config/wofi
/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME add ~/.config/starship.toml
/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME add ~/.local/bin
/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME commit -m "$(date)"
